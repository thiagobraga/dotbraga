# Reset
Color_Off='\033[0m' # Text Reset
# echo $Color_Off Teste $Color_Off

# Regular Colors
Black='\033[0;30m' # Black
# echo $Black Teste $Color_Off
Red='\033[0;31m' # Red
# echo $Red Teste $Color_Off
Green='\033[0;32m' # Green
# echo $Green Teste $Color_Off
Yellow='\033[0;33m' # Yellow
# echo $Yellow Teste $Color_Off
Blue='\033[0;34m' # Blue
# echo $Blue Teste $Color_Off
Purple='\033[0;35m' # Purple
# echo $Purple Teste $Color_Off
Cyan='\033[0;36m' # Cyan
# echo $Cyan Teste $Color_Off
White='\033[0;37m' # White
# echo $White Teste $Color_Off

# Bold
BBlack='\033[1;30m' # Black
# echo $BBlack Teste $Color_Off
BRed='\033[1;31m' # Red
# echo $BRed Teste $Color_Off
BGreen='\033[1;32m' # Green
# echo $BGreen Teste $Color_Off
