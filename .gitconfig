[core]
  editor = code --wait
  fileMode = false
[diff]
  tool = default-difftool
[difftool "default-difftool"]
  cmd = code --wait --diff $LOCAL $REMOTE
[pull]
  rebase = true
[advice]
  pushNonFastForward = false
  statusHints = false
  commitBeforeMerge = false
  resolveConflict = false
  detachedHead = false
[include]
  path = ~/www/.gitconfig
